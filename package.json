{
  "name": "weatherclock",
  "version": "0.0.0",
  "homepage": "https://walles.github.io/weatherclock",
  "repository": {
    "type": "git",
    "url": "https://github.com/walles/weatherclock.git"
  },
  "bugs": {
    "url": "https://github.com/walles/weatherclock/issues"
  },
  "author": "Johan Walles <johan.walles@gmail.com>",
  "private": false,
  "dependencies": {
    "@material-ui/core": "4.4.2",
    "gh-pages": "^5.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-page-visibility": "^4.1.1",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "start": "VITE_GIT_SHA=$(git rev-parse HEAD) vite",
    "build": "VITE_GIT_SHA=$(git rev-parse HEAD) vite build",
    "preview": "vite preview",
    "lint": "eslint src/*.js* && eslint src/*.ts*",
    "test": "echo 'No test runner configured'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "airbnb-typescript",
      "airbnb/hooks"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "consistent-return": 2,
      "no-var": 1,
      "curly": 1
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.5.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "vite": "^6.3.5"
  }
}